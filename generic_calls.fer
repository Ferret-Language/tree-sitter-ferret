constraint numeric = union {
    i32, i64
};

type Pair<T, U> struct {
    .left: T,
    .right: U
};

fn add<T: numeric>(a: T, b: T) -> T {
    return a + b;
}

fn pickFirst<T, U>(a: T, b: U) -> T {
    return a;
}

fn main() -> i32 {
    let inferred := add(10, 20);
    let explicit := add<i64>(1 as i64, 2 as i64);
    let chosen := pickFirst<i32, str>(inferred, "ok");
    let ignored := explicit;
    return chosen;
}
